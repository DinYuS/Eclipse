package com.ali.dao;

import java.util.List;

import com.ali.domain.User;

/*
 * 用来操作用户的DAO接口
 */
public interface UserDao {
	
	User findUserByUsernameAndPassword(String username,String password);
	
	void add(User user);
	void update(User user);
	void delete(int id);
	User findById(int id);
	List<User> findAll();
	
	
	
}
